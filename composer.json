{
    "name": "383project/nova-google2fa",
    "description": "This package provides Google2FA to Laravel Nova.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "pragmarx/google2fa-laravel": "^1.0",
        "pragmarx/google2fa-qrcode": "^1.0",
        "pragmarx/recovery": "^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Project383\\Google2fa\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Project383\\Google2fa\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
